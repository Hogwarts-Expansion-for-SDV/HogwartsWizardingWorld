  "obj.artifacts.CloakofInvisibility.desc": "A magical artefact used to render the wearer invisible.",
  "obj.artifacts.ElderWand.name": "Elder Wand",
  "obj.artifacts.ElderWand.desc": "The most powerful wand ever to exist.",
  "obj.artifacts.GoldenSnitch.name": "Golden Snitch",
  "obj.artifacts.GoldenSnitch.desc": "The third and smallest ball used in Quidditch.",
  "obj.artifacts.HarryPottersGlasses.name": "Harry Potter's Glasses",
  "obj.artifacts.HarryPottersGlasses.desc": "Round spectacles worn by Harry Potter.",
  "obj.artifacts.HogwartsAcceptanceLetter.name": "Hogwarts Acceptance Letter",
  "obj.artifacts.HogwartsAcceptanceLetter.desc": "A letter of acceptance to Hogwarts School of Witchcraft and Wizardry.",
  "obj.artifacts.HufflepuffsCup.name": "Hufflepuff's Cup",
  "obj.artifacts.HufflepuffsCup.desc": "A magical item created by Helga Hufflepuff.",
  "obj.artifacts.MaraudersMap.name": "Marauder's Map",
  "obj.artifacts.MaraudersMap.desc": "A magical document that revealed all of Hogwarts School of Witchcraft and Wizardry.",
  "obj.artifacts.MarvoloGauntsRing.name": "Marvolo Gaunt's Ring",
  "obj.artifacts.MarvoloGauntsRing.desc": "An heirloom of the House of Gaunt.",
  "obj.artifacts.PhilosophersStone.name": "Philosopher's Stone",
  "obj.artifacts.PhilosophersStone.desc": "A legendary alchemical substance with magical properties.",
  "obj.artifacts.RavenclawsDiadem.name": "Ravenclaw's Diadem",
  "obj.artifacts.RavenclawsDiadem.desc": "The only known relic once belonging to Rowena Ravenclaw.",
  "obj.artifacts.ResurrectionStone.name": "Resurrection Stone",
  "obj.artifacts.ResurrectionStone.desc": "One of the fabled Deathly Hallows.",
  "obj.artifacts.SlytherinsLocket.name": "Slytherin's Locket",
  "obj.artifacts.SlytherinsLocket.desc": "A piece of jewellery originally owned by Salazar Slytherin.",
  "obj.artifacts.SortingHat.name": "Sorting Hat",
  "obj.artifacts.SortingHat.desc": "A sentient magical hat at Hogwarts that determined which of the four school Houses each new student belonged most to.",
  "obj.artifacts.SwordofGryffindor.name": "Sword of Gryffindor",
  "obj.artifacts.SwordofGryffindor.desc": "A thousand-year-old, goblin-made magical sword owned by the famed wizard Godric Gryffindor.",
  "obj.artifacts.TimeTurner.name": "Time-Turner",
  "obj.artifacts.TimeTurner.desc": "A magical device used for time travel.",
